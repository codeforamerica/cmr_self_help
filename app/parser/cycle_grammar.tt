grammar CycleGrammar
  rule root
    event_content (event_delimiter event_content)* <Cycle>
  end

  rule event_content
    (!event_delimiter .)* <EventContent>
  end

  rule event_delimiter
     ( N w [-] [- \t]* N ) / ( N &court_identifier)
  end

  rule court_identifier
    'COURT' w ':'
  end

  rule W
    [ \t]+
  end

  rule N
    [\n]+
  end

  rule w
   W?
  end

  rule wn
    [ \t\n]*
  end
end
